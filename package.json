    "babel-register": "^6.3.13",
    "css-loader": "^0.23.0",
    "espower-babel": "^4.0.0",
    "html-loader": "^0.4.0",
    "mocha": "^2.3.4",
    "node-libs-browser": "^0.5.3",
    "node-sass": "^3.4.2",
